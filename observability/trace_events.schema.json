{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Trace Event",
  "type": "object",
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "service": {
      "type": "string"
    },
    "level": {
      "type": "string",
      "enum": [
        "info",
        "warning",
        "error"
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "timestamp",
    "service",
    "level",
    "message"
  ]
}